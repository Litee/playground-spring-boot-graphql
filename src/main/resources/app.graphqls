type Query {
    books: [Book!]
    authors: [Author!]
    book(id: Int!): Book
    author(id: Int!): Author
}

type Mutation {
    changeBookTitle(id: Int!, newTitle: String!): Book
}

# Books in my library
type Book {
    id: Int!
    title: String!
    authors: [Author!]
}

type Author {
    id: Int!
    name: String!
    books: [Book!]
}